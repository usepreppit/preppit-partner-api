{
  "name": "Usepreppit_API",
  "version": "0.0.1",
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "bin": {
    "nodejs-typescript-modern-starter": "dist/src/index.js",
    "ntms": "dist/src/index.js"
  },
  "repository": {
    "type": "git",
