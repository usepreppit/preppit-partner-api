  "scripts": {
    "bundle": "rimraf dist && esbuild --outdir=dist --sourcemap --bundle --platform=node --target=node20.10.0 ./src/index.ts",
    "compile": "tsc",
    "compile:watch": "tsc -w",
    "dev": "nodemon --watch src --watch test --ext ts,json",
    "vercel-dev": "node src/index.ts",
    "build": "tsc",
    "ts.check": "tsc --project tsconfig.json",
    "add-build": "git add dist",
    "debug": "yarn bundle && node --enable-source-maps --inspect-brk ./dist/index.js",
    "debug:test": "node --inspect-brk ./node_modules/.bin/jest --runInBand .",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "start": "node dist/index.js",
    "build:prod": "tsc && node dist/index.js",
    "start:prod": "node dist/index.js",
    "test": "jest --runInBand .",
    "serve": "node dist/index.js"
  },
